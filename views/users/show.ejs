<div>
  <%- include('_nav_links') %> 
</div>
<div class="ag-show-ag-row">
  <h2 class="page-header"> <%= targetUser.name %> - AG </h2>
  <div>
    <%= targetUser.contactMail %> 
  </div>
</div>
<div>
  <div class="text-bright">
    <%- targetUser.description %> 
  </div>
  <div>
    <h4>Arbeitsstand:</h4>
    <div class="text-bright">
      <%- targetUser.statusOfWork %> 
    </div>
  </div>
</div>
<div class="ag-show-posts">
  <h3>Protokolle der <%= targetUser.name %> - AG</h3>   
  <% posts.forEach(post => { %> 
    <div class="ag-show-post">
      <div class="ag-post-name">
        <a href="/posts/<%= post.id %> "><%= post.title %></a>
      </div>
      <div class="ag-post-comments-count">
        <%  let postComments = [] %> 
        <% postsComments.forEach(postsComment => { %>  
          <% if (postsComment.post == post.id) { postComments.push(postsComment) }%>
        <% }) %>
        Kommentare: <%= postComments.length %> 
      </div>
    </div>
  <% }) %>
</div>
<div>
  <%- include('_nav_links') %> 
</div>