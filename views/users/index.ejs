<h2 class="page-header">Alle AGs</h2>
<div class="page-container">
  <% targetUsers.forEach(targetUser => { %>
    <div class="ag-container">
      <div class="ag-name">
        <a class="btn btn-primary" href="users/<%=targetUser.id%>"><%= targetUser.name %></a>
      </div>
      <div class="ag-posts">
        <p>Neueste Beitr√§ge:</p>
        <% let agPosts = [] %> 
        <% posts.forEach(post => { %>
          <% if (post.user == targetUser.id) { %>
            <% agPosts.push(post) %> 
          <% } %>
        <% }) %>
        <% agPosts.slice(0, 2).forEach(agPost => { %> 
          <div class="ag-posts-title">
            <a href="/posts/<%= agPost.id %>"><%= agPost.title %></a>
          </div>
          <div class="ag-post-date">
            <% let postDate = agPost.createdAt %> 
            Erstellt am: <%= `${postDate.getDate()}.${postDate.getMonth()}.${postDate.getFullYear()}` %>
          </div>
        <% }) %> 
      </div>
      <div class="ag-info">
        <div class="ag-info-content">
          Kontakt: <%= targetUser.contactMail %> 
        </div>
        <div class="ag-info-content">
          <% let agDate = targetUser.registerDate %> 
          Beigetreten am: <%= `${agDate.getDate()}.${agDate.getMonth()}.${agDate.getFullYear()}` %> 
        </div>
      </div>
    </div>
  <%  })  %> 
</div>